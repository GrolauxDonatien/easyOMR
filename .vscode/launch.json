{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch application",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
            },
            "args": [
                "."
            ],
            "outputCapture": "std"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "MSI Builder",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\build_installer.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\test.js"
        },
        {
            "name": "Export train data",
            "description":"Some scripts require librairies of easyOMR, running them through electron ensures compatibility",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
            },
            "args": [
                ".",
                "predict",
                "out"
            ],
            "args creating training data": [
                ".",
                "trainingdata",
                "projectSrc",
                "trainingDst"
            ],
            "args for running another script": [
                ".",
                "batchinvert",
                "directory"
            ],
            "outputCapture": "std"
        }
    ]
}